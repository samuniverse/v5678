{
  "vpn": {
    "enabled": false,
    "clientType": "manual",
    "command": "",
    "connectionVerifyUrl": "https://www.google.com",
    "ipTrackingEndpoint": "https://api.ipify.org?format=json",
    "connectionVerifyTimeout": 5000,
    "maxRetries": 10,
    "retryDelay": 2000,
    "changeAfterFailures": 5,
    "rotationStrategy": "manual",
    "rotationCount": 500,
    "rotationIntervalMs": 3600000,
    "serverList": []
  },
  "waitTimes": {
    "scrollDelay": 1000,
    "minVariance": 2000,
    "maxVariance": 5000
  },
  "scraping": {
    "concurrency": 2,
    "maxConcurrency": 4,
    "canvasConcurrency": 2,
    "maxRetryRounds": 2,
    "retryDelay": 5000,
    "detectEmptyResults": true,
    "orderedSequential": false,
    "interTabDelayMin": 0,
    "interTabDelayMax": 0,
    "ensureTabActive": true,
    "pageRecreationInterval": 5,
    "dynamicPoolSizing": true,
    "minPoolSize": 1,
    "maxPoolSize": 4,
    "maxTasksPerProcess": 1,
    "processRecyclingEnabled": true,
    "memoryThresholdMB": 300
  },
  "metadata": {
    "metadataTimeout": 15000,
    "cookieBannerSelector": ".cky-btn.cky-btn-accept"
  },
  "navigation": {
    "timeout": 60000,
    "waitUntil": "domcontentloaded",
    "maxConcurrentJobs": 3
  },
  "smartframe": {
    "extractFullImages": false,
    "viewportMode": "thumbnail",
    "headless": false,
    "enableParallelCanvasExtraction": true,
    "renderTimeout": 5000,
    "initialRenderWaitMs": 15000,
    "postResizeWaitMs": 8000,
    "maxRenderWaitMs": 120000,
    "clientSideStage1DelayMs": 12000,
    "clientSideStage2DelayMs": 8000,
    "clientSideThumbnailDelayMs": 3000,
    "clientSideFallbackDelayMs": 5000,
    "tabActivationIntervalMs": 500,
    "gpuRenderWindowMs": 15000,
    "maxConcurrentRenders": 1,
    "renderTelemetryEnabled": false,
    "workerPoolSize": 2,
    "jpgQuality": {
      "thumbnail": 82,
      "full": 92
    },
    "imageFormat": {
      "thumbnail": "webp",
      "full": "progressive-jpeg"
    },
    "enableChecksumValidation": true,
    "checksumSampleSize": 100,
    "enableTiledFallback": true,
    "tiledFallbackGridSize": 2,
    "archiveOriginalPNG": true,
    "archiveSizeThreshold": 102400,
    "minValidFileSize": 51200,
    "minValidDimensions": 500,
    "enableVarianceCheck": true,
    "viewportSizes": {
      "full": {
        "width": 9990,
        "height": 9990,
        "deviceScaleFactor": 1
      },
      "thumbnail": {
        "width": 600,
        "height": 600,
        "deviceScaleFactor": 1
      },
      "large": {
        "width": 1920,
        "height": 1080,
        "deviceScaleFactor": 1
      }
    },
    "resizeSteps": {
      "enabled": false,
      "steps": ["thumbnail", "large", "full"],
      "delayBetweenSteps": 2000
    }
  },
  "caption": {
    "enableMultiParagraph": true,
    "supportedLocales": ["en", "es", "fr", "de"],
    "defaultLocale": "en"
  }
}
